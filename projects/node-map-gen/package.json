{
  "name": "node-typescript-boilerplate",
  "description": "Minimalistic boilerplate to quick-start Node.js development in TypeScript.",
  "license": "Apache-2.0",
  "author": "Jakub Synowiec <github@jakubsynowiec.info>",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "check-deps": "depcheck --skip-missing --ignore-bin-package --ignores $(cat .depcheckignore)",
    "clean": "rimraf coverage build tmp",
    "lint": "tslint -t stylish -p tsconfig.json",
    "lint:test": "tslint -t stylish -p tsconfig.test.json -c tslint.test.json",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "chance": "^1.0.18",
    "minimist": "^1.2.0",
    "ramda": "^0.26.1",
    "ramda-adjunct": "^2.17.0",
    "utility-types": "^3.6.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chalk": "^2.2.0",
    "@types/chance": "^1.0.2",
    "@types/jest": "^24.0.12",
    "@types/minimist": "^1.2.0",
    "@types/node": "10",
    "@types/ramda": "types/npm-ramda#dist",
    "@types/strip-ansi": "^5.2.1",
    "depcheck": "^0.7.2",
    "expect-more-jest": "^2.4.1",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "jest-circus": "^24.8.0",
    "jest-extended": "^0.11.1",
    "jest-watch-suspend": "^1.1.1",
    "jest-watch-toggle-config": "^1.0.2",
    "jest-watch-typeahead": "^0.3.1",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0",
    "prettier-package-json": "^2.1.0",
    "pretty-quick": "^1.10.0",
    "rimraf": "^2.6.3",
    "strip-ansi": "^5.2.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "~6.1.1",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">= 10.13 <11"
  }
}
