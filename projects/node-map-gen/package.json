{
  "private": true,
  "name": "node-map-gen",
  "version": "0.0.0",
  "scripts": {
    "pre-commit": "pretty-quick --staged && lint-staged",
    "pre-push": "(yarn lint & tsc --noEmit) && jest --ci",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "check-deps": "depcheck --skip-missing --ignore-bin-package --ignores $(cat .depcheckignore) || true",
    "clean": "rimraf coverage build tmp",
    "lint": "tslint -t stylish -p tsconfig.json",
    "lint:test": "tslint -t stylish -p tsconfig.spec.json -c tslint.test.json",
    "serve": "nodemon -e ts -w src --exec 'ts-node -r tsconfig-paths/register -T src/server.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "chance": "^1.0.18",
    "common-tags": "^1.8.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "minimist": "^1.2.0",
    "ramda": "^0.26.1",
    "ramda-adjunct": "^2.17.0",
    "strip-ansi": "^5.2.0",
    "tsconfig-paths": "^3.8.0",
    "utility-types": "^3.6.1",
    "yaml": "^1.5.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chalk": "^2.2.0",
    "@types/chance": "^1.0.2",
    "@types/common-tags": "^1.8.0",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.12",
    "@types/minimist": "^1.2.0",
    "@types/node": "10",
    "@types/ramda": "types/npm-ramda#dist",
    "@types/strip-ansi": "^5.2.1",
    "@types/yaml": "^1.0.2",
    "depcheck": "^0.7.2",
    "expect-more-jest": "^2.4.1",
    "jest": "^24.8.0",
    "jest-circus": "^24.8.0",
    "jest-extended": "^0.11.1",
    "jest-watch-suspend": "^1.1.1",
    "jest-watch-toggle-config": "^1.0.2",
    "jest-watch-typeahead": "^0.3.1",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0",
    "prettier-package-json": "^2.1.0",
    "pretty-quick": "^1.10.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "~6.1.1",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">= 10.13 <11"
  }
}
